@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<table class=" table table-boardered-stripped" style=" width:100%">
    <thead>
        <tr>
           <th>
               Id
           </th>
           <th>
               Category Name
           </th>
           <th>
               Dispaly Order
           </th>
           <th>
               CreatedDateTime
           </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
          <tr>
              <td width="30%">
                  @obj.Id
              </td>
                <td width="30%">
                    @obj.Name
                </td>
                    <td width="30%">
                    @obj.DisplayOrder
                  </td>
                      <td width="30%">
                    @obj.CreatedDateTime
                  </td>
          </tr>  
        }
    </tbody>
</table>

